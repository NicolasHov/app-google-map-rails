<head>
    <title>Simple Map</title>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
   
  </head>
  <body>

    <div class="container">
        <%= form_tag('/get') do %>
        <%= label_tag(:latit, "Lattitude:") %>
        <%= text_field_tag(:latit) %>
        <%= label_tag(:longit, "Longitude:") %>
        <%= text_field_tag(:longit) %>
        <%= submit_tag("Search") %>
      <% end %>
    </div>

    <div id="map"></div>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDAUarZPNMdG9EjKdGuKvcUyovLI3LdvkQ&callback=initMap"
async defer></script>

  </body>